print("SUM OF N NUMBER")
print("---------------")
a=int(input("Enter the starting number:"))
b=int(input("Enter the ending number:"))
c=int(input("Enter the different:"))
print("Result")
print("------")
count=0
sum=0
for i in range(a,b,c):
    print("+",+i)
    sum=sum
    count=i
    print("sum",sum)
    print("count:",count)
